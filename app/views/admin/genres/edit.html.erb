<div class="countainer">
  <h3>ジャンル一覧・追加</h3>
  <%= form_with model: @genre, url:admin_genre_path(@genre) do |f| %>
    <div class="form-inline">
      <%= f.label :name, 'ジャンル名' %>
      <%= f.text_field :name, placeholder: "ジャンル名", class: 'form-control' %>
      <%= f.submit '変更を保存',class: 'btn btn-success' %>
    </div>
  <% end %>
</div>